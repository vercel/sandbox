{
  "name": "sandbox",
  "description": "Command line interface for Vercel Sandbox",
  "version": "2.3.0",
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "sandbox": "ts-node ./src/sandbox.ts",
    "build": "tsdown",
    "vercel-build": "ts-node ./scripts/create-preview-deployment-tgz.ts",
    "test": "vitest",
    "example-whoami": "ts-node ./src/example-whoami.ts",
    "example-next": "ts-node ./src/example-next.ts",
    "example-flask": "ts-node ./src/example-flask.ts",
    "typecheck": "tsc --noEmit",
    "generate-docs": "ts-node ./scripts/print-usage.ts > docs/index.md",
    "version:bump": "pnpm generate-docs"
  },
  "keywords": [
    "vercel",
    "sandbox",
    "cli"
  ],
  "main": "./dist/index.mjs",
  "bin": {
    "sandbox": "./bin/sandbox.mjs",
    "sbx": "./bin/sandbox.mjs"
  },
  "files": [
    "bin",
    "README.md",
    "dist"
  ],
  "homepage": "https://vercel.com/docs/vercel-sandbox/cli-reference",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/sandbox.git",
    "directory": "packages/sandbox"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@vercel/sandbox": "workspace:*",
    "debug": "^4.4.1",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.15.12",
    "@vercel/oidc": "^3.1.0",
    "@vercel/pty-tunnel": "workspace:*",
    "@vercel/pty-tunnel-server": "workspace:*",
    "@vercel/sandbox": "workspace:*",
    "chalk": "^5.6.0",
    "cmd-ts": "0.15.0",
    "date-fns": "^4.1.0",
    "dotenv-flow": "^4.1.0",
    "listr2": "^9.0.2",
    "ms": "^2.1.3",
    "open": "^10.2.0",
    "ora": "^8.2.0",
    "tsdown": "catalog:",
    "vitest": "catalog:",
    "xdg-app-paths": "5.1.0"
  }
}
